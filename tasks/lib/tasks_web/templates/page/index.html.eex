<h1>Log In</h1>
<%= form_tag("/session", method: :post) do %>
  <div class="form-group">
    <input type="email" name="email" placeholder="Email">
  </div>
  <div class="form-group">
    <button class="btn btn-primary">Log in</button>
  </div>
  <div class="form-group">
    <%= link "Sign up", to: user_path(@conn, :new) %>
  </div>
<% end %>
